$mainMod = SUPER

bind = $mainMod, C, killactive,
bind = , mouse:276, killactive,
bind = CTRL ALT, delete, exit,
bind = $mainMod, P, pseudo, # dwindle
bind = $mainMod, space, togglesplit, # dwindle
bind = $mainMod CTRL, V, togglefloating,
bind = $mainMod, semicolon, exec, hyprlock

source = $configDir/keybindings/programs.hyprland
source = $configDir/keybindings/window.hyprland
source = $configDir/keybindings/workspace.hyprland
source = $configDir/keybindings/media.hyprland
